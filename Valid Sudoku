class Solution {
    public boolean isValidSudoku(char[][] board) {
        HashSet<String> hs = new HashSet<>();
        for(int i=0;i<9;i++)
            for(int j = 0;j<9;j++)
                if(board[i][j]!='.')
                {
                    if(!hs.add(board[i][j] + "found at row : " + i)||
                       !hs.add(board[i][j] + "found at column: "+j ) ||
                       !hs.add(board[i][j] + "found at subBox: "+ i/3 +":"+j/3))
                            return false;
                }
                               
        return true;
    }
}
